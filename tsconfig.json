{
	"compilerOptions": {
		"allowJs": true,
		"baseUrl": "./",
		"outDir": "./dist",
		"jsx": "react",
		"jsxFactory": "Suiact.createElement",
		"jsxFragmentFactory": "Suiact.Fragment",
		"target": "ES3",
		"noLib": true,
		"ignoreDeprecations": "5.0",
		"types": [
			"types-for-adobe/shared/global"
		]
	},
	"include": [
		"./src/**/*"
	],
}